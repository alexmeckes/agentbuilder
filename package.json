{
  "name": "any-agent-workflow-composer",
  "version": "1.0.0",
  "description": "Visual workflow composer for any-agent multi-agent orchestration",
  "scripts": {
    "start": "./start.sh",
    "stop": "./stop.sh",
    "dev": "./dev.sh",
    "logs": "tail -f backend.log frontend.log",
    "logs:backend": "tail -f backend.log",
    "logs:frontend": "tail -f frontend.log",
    "status": "lsof -i :3000,3001,8000 || echo 'No services running'",
    "test:backend": "curl -s http://localhost:8000/ | jq .",
    "test:frontend": "curl -s http://localhost:3000/ | head -3"
  },
  "keywords": [
    "ai",
    "agents",
    "workflow",
    "orchestration",
    "visual",
    "composer"
  ],
  "author": "any-agent team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.8.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0"
  }
}
